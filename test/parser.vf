struct Point {
  int x;
  int y;

  Point add(self, Point other) {
    Point o = Point();
    o.x = self.x + other.x;
    o.y = self.y + other.y;
    return o;
  }

  int dist(self) {
    return self.x * self.x + self.y * self.y;
  }

  int to_string() {}
}

int add(int a, int b) {
  return a + b;
}

