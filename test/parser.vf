struct Point {
  x: Int;
  y: Int;
  
  def add(self, other: Point) -> Point {
    let o: Point = Point();
    //o.x = self.x + other.x;
    //o.y = self.y + other.y;
    o = self.x + other.x;
    o = self.y + other.y;
    return o;
  }

  def dist(self) -> Int {
    return self.x * self.x + self.y * self.y;
  }
}

def add(a: Int, b: Int) -> Int {
  return a + b;
}

