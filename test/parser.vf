struct Point {
  x: Int;
  y: Int;
  
  def add(self, other: Point) -> Point {
    Point o = Point();
    o.x = self.x + other.x;
    o.y = self.y + other.y;
    return o;
  }

  def dist(self) -> Int {
    return self.x * self.x + self.y * self.y;
  }
}

def add(a: Int, b: Int) -> Int {
  return a + b;
}

