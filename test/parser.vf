struct Point {
  x: Int;
  y: Int;
  
  def add(self, Point other) -> Point {
    Point o = Point();
    o.x = self.x + other.x;
    o.y = self.y + other.y;
    return o;
  }

  def dist(self) -> Int {
    return self.x * self.x + self.y * self.y;
  }
}

int add(int a, int b) {
  return a + b;
}

